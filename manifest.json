{
  "manifest_version": 2,
  "name": "Peterson Calendar",
  "description": "A Google Calendar extension for planning out one's life.",
  "version": "1.0.0",
  "icons": {"128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "identity", "https://www.googleapis.com/"],
  "background": {
     "scripts": [
       "popup.js"
     ]
   },
  "oauth2": {
    "client_id": "177939000497-3om91jrt4i71itiaicbs3jhgtq52ug07.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  }
}
