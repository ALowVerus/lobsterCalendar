{
  "manifest_version": 2,
  "name": "Lobster Calendar",
  "description": "A Google Calendar extension for planning out one's life.",
  "version": "0.0.0",
  "icons": {"128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app.html"
  },
  "permissions": [
    "identity",
    "storage",
    "https://www.googleapis.com/"
  ],
  /* "background": {
     "scripts": [
       "app.js"
     ]
   }, */
  "oauth2": {
    "client_id": "XXXXXXXX.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}
